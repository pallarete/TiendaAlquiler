@model TiendaAlquiler.Models.Decada

@{
    ViewData["Title"] = "Delete";
}

<h1>Borrar</h1>

<h3>Esta seguro de borrar esta decada?</h3>
<div>
    <h4>Decada</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AnioInicio)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AnioInicio)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="DecadaId" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Volver al listado</a>
    </form>
</div>
